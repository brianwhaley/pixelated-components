{
    "env": {
        "es2021": true,
        "node": true,
		"browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		"indent": [2, "tab"],
        "no-tabs": 0
    }
}
