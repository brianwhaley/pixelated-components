{
  "name": "pixelated-admin",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "dev": "npm run clean && NODE_OPTIONS='--inspect' next dev -p 3006 --experimental-https",
    "build": "npm run clean && NODE_ENV=production next build",
    "start": "npm run clean && next build && next start",
    "lint": "eslint --fix",
    "clean": "rm -rf .next && rm -rf out",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "generate-site-images": "node ./node_modules/@pixelated-tech/components/dist/scripts/generate-site-images.js",
    "config:vault": "npx tsx ./node_modules/@pixelated-tech/components/dist/scripts/config-vault.ts",
    "config:encrypt": "npm run config:vault -- encrypt src/app/config/pixelated.config.json",
    "config:decrypt": "npm run config:vault -- decrypt src/app/config/pixelated.config.json",
    "release": "./node_modules/@pixelated-tech/components/dist/scripts/release.sh"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@aws-sdk/client-cloudwatch": "^3.966.0",
    "@aws-sdk/client-route-53": "^3.966.0",
    "@pixelated-tech/components": "^3.9.0",
    "axe-core": "^4.11.1",
    "googleapis": "^170.0.0",
    "next": "^16.1.1",
    "next-auth": "^4.24.7",
    "puppeteer": "^24.34.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "sass": "^1.97.2",
    "ssh2": "^1.17.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ssh2": "^1.15.5",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "^27.4.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
