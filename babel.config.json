{
	"presets" : [
		[
			"@babel/preset-env", { 
				"modules": false,
				"targets": {
					// "browsers": ["> 0.2%", "not dead"],
					"esmodules": true,
					"node": "current"
				},
				"bugfixes": true,
				"useBuiltIns": "usage",
				"corejs": "3.6.5"
			}
		],
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	],
}
